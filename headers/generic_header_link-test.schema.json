{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "https://github.com/ITxPT/schematest/blob/main/headers/generic_header_link-test.schema.json",
	"title": "The generic header that all ITxPT schema's shall include",
	"description": "ITxPT generic header with schema version and github pointer",
	"type": "object",
	"required": [
		"$schema",
		"version",
		"atDateTime"
	],
	"properties": {
		"version": {
			"type": "string",
			"minLength": 5,
			"maxLength": 14,
			"pattern": "^(?:0|[1-9]\\d*)\\.(?:0|[1-9]\\d*)\\.(?:0|[1-9]\\d*)$"
		},
		"atDateTime": {
			"type": "string",
			"format": "date-time"
		}
	}
}
